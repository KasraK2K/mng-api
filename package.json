{
  "name": "nodejs_starter",
  "version": "1.0.0",
  "description": "This is a node.js starter pack to create service easy and fast",
  "main": "src/index.ts",
  "scripts": {
    "start": "cross-env NODE_ENV=production node build/index.js",
    "start:dev": "cross-env NODE_ENV=development ts-node run.ts",
    "prebuild": "rimraf build",
    "build:dev": "yarn prebuild && cross-env NODE_ENV=development webpack -- --color",
    "build": "yarn prebuild && cross-env NODE_ENV=production webpack -- --color",
    "test": "jest"
  },
  "keywords": [
    "nodejs",
    "express",
    "starter",
    "boilerplate"
  ],
  "author": "Kasra Karami <Kasra.Karami.Work@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "ajv": "^8.10.0",
    "ajv-formats": "^2.1.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.0",
    "dotenv-expand": "^8.0.1",
    "express": "^4.17.3",
    "google-libphonenumber": "^3.2.27",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.21",
    "mongodb": "^4.4.1",
    "mongoose": "^6.2.3",
    "os": "^0.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@types/express": "^4.17.13",
    "@types/google-libphonenumber": "^7.4.23",
    "@types/jest": "^27.4.1",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/lodash": "^4.14.178",
    "@types/node": "^17.0.18",
    "babel-jest": "^27.5.1",
    "jest": "^27.5.1",
    "nodemon": "^2.0.15",
    "npm": "^8.5.1",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5",
    "webpack": "^5.69.1",
    "webpack-cli": "^4.9.2",
    "webpack-node-externals": "^3.0.0",
    "webpack-shell-plugin-next": "^2.2.2"
  }
}
